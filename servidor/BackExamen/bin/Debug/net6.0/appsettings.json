{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CorsOptions": {
    "DefaultPolicy": {
      "Origins": [ "*" ],
      "Methods": [ "GET", "POST", "PUT", "DELETE" ],
      "Headers": [ "Content-Type", "Accept" ]
    }
  }
}
